    "frames-react": "^1.0.2",
    "gray-matter": "^4.0.3",
    "jszip": "^3.7.1",
    "launchdarkly-react-client-sdk": "^2.23.1",
    "next": "^12.0.3",
    "next-absolute-url": "^1.2.2",
    "next-mdx-remote": "^3.0.2",
