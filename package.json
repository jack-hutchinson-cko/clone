{
  "name": "checkout-docs-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create-index-for-algolia": "ts-node -T -P tsconfig.create-algolia-index.json -r tsconfig-paths/register ./tools/createIndexForAlgolia.tsx",
    "create-site-map": "ts-node -T -P tsconfig.create-algolia-index.json -r tsconfig-paths/register ./tools/createSiteMap.tsx",
    "dev": "next dev",
    "build": "npm run prebuild; next build",
    "prebuild": "npm run git-unshallow",
    "start": "next start",
    "dev:abc": "NEXT_PUBLIC_HEAP_IO_ACCOUNT=ABC NEXT_PUBLIC_CLIENT_TYPE=ABC next dev",
    "dev:nas": "NEXT_PUBLIC_HEAP_IO_ACCOUNT=NAS NEXT_PUBLIC_CLIENT_TYPE=NAS next dev",
    "cy:run-units": "cypress run-ct --config-file=cypress.units.json",
    "cy:open-units": "cypress open-ct --config-file=cypress.units.json",
    "cy:run-components": "cypress run-ct --config-file=cypress.components.json",
    "cy:open-components": "cypress open-ct --config-file=cypress.components.json",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "postinstall": "husky install && cypress install",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "chromatic": "chromatic --exit-zero-on-changes",
    "git-unshallow": "git fetch --depth=1000000"
  },
  "dependencies": {
    "@cko/icons": "^3.17.2",
    "@cko/primitives": "^5.3.0",
    "@cko/utils": "^1.1.9",
    "@datadog/browser-logs": "^3.6.13",
    "@datadog/browser-rum": "^3.0.1",
    "@mdx-js/runtime": "^1.6.22",
    "@storybook/react": "^6.4.0-alpha.22",
    "@types/lodash": "^4.14.170",
    "algoliasearch": "^4.10.2",
    "classnames": "^2.3.1",
